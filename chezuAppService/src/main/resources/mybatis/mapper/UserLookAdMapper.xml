<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.idata365.app.mapper.UserLookAdMapper">


    <select id="getTodayCount">
        SELECT count(1) FROM userLookAdLogs WHERE userId = #{userId} and daystamp=#{daystamp}
    </select>

    <insert id="insertLogs" parameterType="com.idata365.app.entity.v2.UserLookAdLogs" >
       INSERT INTO 
            userLookAdLogs
            (userId,createTime,daystamp,remark,powerNum) 
        VALUES
            (#{userId}, #{createTime}, #{daystamp}, #{remark}, #{powerNum})
    </insert>

</mapper>
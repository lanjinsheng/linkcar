<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.idata365.app.mapper.AssetFamiliesAssetMapper">
    <resultMap id="BaseFamiliestMap" type="com.idata365.app.entity.AssetFamiliesAsset" >
    </resultMap>
 
     <update id="updatePowerAdd"   parameterType="com.idata365.app.entity.AssetFamiliesPowerLogs">

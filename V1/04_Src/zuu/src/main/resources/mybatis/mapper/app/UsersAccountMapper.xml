<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.idata365.mapper.app.UsersAccountMapper">
 

     <select id="getUserIdByToken" resultType="java.lang.Long" parameterType="java.lang.String">
       SELECT userId
       FROM  userLoginSession where token=#{token}
     </select>
    
    
</mapper>
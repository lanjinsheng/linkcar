<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.idata365.app.mapper.DicNotifyMapper">
    <select id="getLatestNotify" resultType="java.util.HashMap">
 		select notifyText  from dicNotify order by id desc limit 1
 	</select>
    
    <select id="playHelper" resultType="java.util.HashMap">
 		select *  from dicPlayHelp
 	</select>
</mapper>
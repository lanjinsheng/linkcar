<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.idata365.mapper.app.DriveDataEventMapper">
   <insert id="insertDriveEvent" parameterType="java.util.HashMap">
		 INSERT INTO  driveDataEvent (driveDataMainId,alarmType,alarmValue,lat,lng,startTime,endTime) VALUES
    	<foreach collection="list" item="item" index="index" separator=",">
    		(#{driveDataMainId},#{item.alarmType},#{item.alarmValue},#{item.lat},#{item.lng},#{item.startTime},#{item.endTime}) 
    	</foreach>
	</insert>
</mapper>